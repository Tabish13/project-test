{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType":  "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": 0,
        "valid-jsdoc" : 0,
        "no-unused-vars":1,
        "new-cap":1
    }
}